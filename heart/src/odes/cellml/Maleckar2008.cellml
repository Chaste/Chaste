      <variable name="past" units="second"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>past</ci>
            <apply>
               <times/>
               <apply>
                  <floor/>
                  <apply>
                     <divide/>
                     <ci>time</ci>
                     <ci>stim_period</ci>
                  </apply>
               </apply>
               <ci>stim_period</ci>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>i_Stim</ci>
            <piecewise>
               <piece>
                  <ci>stim_amplitude</ci>
                  <apply>
                     <and/>
                     <apply>
                        <geq/>
                        <apply>
                           <minus/>
                           <ci>time</ci>
                           <ci>past</ci>
                        </apply>
                        <ci>stim_offset</ci>
