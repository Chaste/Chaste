<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<cp:ChasteParameters xmlns:cp="https://chaste.comlab.ox.ac.uk/nss/parameters/2017_1" xmlns:cp20="https://chaste.comlab.ox.ac.uk/nss/parameters/2_0" xmlns:cp21="https://chaste.comlab.ox.ac.uk/nss/parameters/2_1" xmlns:cp22="https://chaste.comlab.ox.ac.uk/nss/parameters/2_2" xmlns:cp23="https://chaste.comlab.ox.ac.uk/nss/parameters/2_3" xmlns:cp30="https://chaste.comlab.ox.ac.uk/nss/parameters/3_0" xmlns:cp31="https://chaste.comlab.ox.ac.uk/nss/parameters/3_1" xmlns:cp33="https://chaste.comlab.ox.ac.uk/nss/parameters/3_3" xmlns:cp34="https://chaste.comlab.ox.ac.uk/nss/parameters/3_4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ChasteParameters_1_1.xsd" xsi:schemaLocation="https://chaste.comlab.ox.ac.uk/nss/parameters/2017_1 ChasteParameters_2017_1.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/2_0 ChasteParameters_2_0.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/2_1 ChasteParameters_2_1.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/2_2 ChasteParameters_2_2.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/2_3 ChasteParameters_2_3.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/3_0 ChasteParameters_3_0.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/3_1 ChasteParameters_3_1.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/3_3 ChasteParameters_3_3.xsd https://chaste.comlab.ox.ac.uk/nss/parameters/3_4 ChasteParameters_3_4.xsd">
  <cp:Simulation>
    <cp:SpaceDimension>3</cp:SpaceDimension>
    <cp:SimulationDuration unit="ms">0.2</cp:SimulationDuration>
    <cp:Domain>Bi</cp:Domain>
    <cp:Mesh unit="cm">
      <cp:Slab inter_node_space="0.01" x="0.1" y="0.1" z="0.1"/>
    </cp:Mesh>
    <cp:IonicModels>
      <cp:Default>
        <cp:Hardcoded>Fox2002BackwardEuler</cp:Hardcoded>
      </cp:Default>
    </cp:IonicModels>
    <cp:Stimuli>
      <cp:Stimulus>
        <cp:Strength unit="uA/cm^3">-80000</cp:Strength>
        <cp:Duration unit="ms">1</cp:Duration>
        <cp:Delay unit="ms">0</cp:Delay>
        <cp:Location unit="cm">
          <cp:Cuboid>
            <cp:LowerCoordinates x="0" y="0" z="0"/>
            <cp:UpperCoordinates x="0.1" y="1" z="1"/>
          </cp:Cuboid>
        </cp:Location>
      </cp:Stimulus>
    </cp:Stimuli>
    <cp:OutputDirectory>SaveBidomainShort</cp:OutputDirectory>
    <cp:OutputFilenamePrefix>SimulationResults</cp:OutputFilenamePrefix>
    <cp:OutputVisualizer cmgui="no" meshalyzer="yes" parallel_vtk="no" precision="0" vtk="no"/>
    <cp:CheckpointSimulation max_checkpoints_on_disk="3" timestep="0.2" unit="ms"/>
  </cp:Simulation>
  <cp:Physiological>
    <cp:IntracellularConductivities longi="1.75" normal="1.75" trans="1.75" unit="mS/cm"/>
    <cp:ExtracellularConductivities longi="7" normal="7" trans="7" unit="mS/cm"/>
    <cp:BathConductivity unit="mS/cm">7</cp:BathConductivity>
    <cp:SurfaceAreaToVolumeRatio unit="1/cm">1400</cp:SurfaceAreaToVolumeRatio>
    <cp:Capacitance unit="uF/cm^2">1</cp:Capacitance>
    <cp:Purkinje>
      <cp:SurfaceAreaToVolumeRatio unit="1/cm">2800</cp:SurfaceAreaToVolumeRatio>
      <cp:Capacitance unit="uF/cm^2">1</cp:Capacitance>
      <cp:Conductivity unit="mS/cm">1.75</cp:Conductivity>
    </cp:Purkinje>
  </cp:Physiological>
  <cp:Numerical>
    <cp:TimeSteps ode="0.01" pde="0.1" printing="0.1" unit="ms"/>
    <cp:KSPTolerances>
      <cp:KSPAbsolute>0.0002</cp:KSPAbsolute>
    </cp:KSPTolerances>
    <cp:KSPSolver>gmres</cp:KSPSolver>
    <cp:KSPPreconditioner>jacobi</cp:KSPPreconditioner>
    <cp:MeshPartitioning>parmetis</cp:MeshPartitioning>
    <cp:UseStateVariableInterpolation>no</cp:UseStateVariableInterpolation>
  </cp:Numerical>
</cp:ChasteParameters>
