      <variable name="E_K" units="millivolt" public_interface="in"/>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
         <apply>
            <eq/>
            <ci>g_K_ATP</ci>
            <apply>
               <divide/>
               <cn cellml:units="milliS_per_microF">0.000193</cn>
               <ci>nicholsarea</ci>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>pATP</ci>
            <apply>
               <divide/>
               <cn cellml:units="dimensionless">1</cn>
               <apply>
                  <plus/>
                  <cn cellml:units="dimensionless">1</cn>
                  <apply>
                     <power/>
                     <apply>
                        <divide/>
                        <ci>ATPi</ci>
                        <ci>kATP</ci>
                     </apply>
                     <ci>hATP</ci>
                  </apply>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>GKbaraATP</ci>
            <apply>
               <times/>
               <ci>g_K_ATP</ci>
               <ci>pATP</ci>
               <apply>
                  <power/>
                  <apply>
                     <divide/>
