                     </apply>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <cn cellml:units="per_second">203.8</cn>
                  <apply>
                     <power/>
                     <apply>
                        <divide/>
                        <ci>Ca_i</ci>
                        <apply>
                           <plus/>
