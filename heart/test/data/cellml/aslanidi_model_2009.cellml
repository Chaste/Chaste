                  <ci>Ca_rel</ci>
                  <apply>
                     <minus/>
                     <cn cellml:units="dimensionless">1</cn>
                     <ci>O_Calse</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <cn cellml:units="per_second">400</cn>
                  <ci>O_Calse</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <apply>
               <diff/>
               <bvar>
                  <ci>time</ci>
               </bvar>
               <ci>Ca_rel</ci>
