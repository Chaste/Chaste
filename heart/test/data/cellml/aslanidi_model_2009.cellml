            <apply>
               <minus/>
               <apply>
                  <divide/>
                  <apply>
                     <minus/>
                     <ci>i_tr</ci>
                     <ci>i_rel</ci>
                  </apply>
                  <apply>
                     <times/>
                     <cn cellml:units="dimensionless">2</cn>
                     <ci>Vol_rel</ci>
                     <ci>F</ci>
                  </apply>
               </apply>
               <apply>
                  <times/>
                  <cn cellml:units="millimolar">31</cn>
                  <apply>
                     <diff/>
                     <bvar>
                        <ci>time</ci>
