                  <ci>i_Kp</ci>
                  <ci>i_NaCa</ci>
                  <ci>i_p_Ca</ci>
                  <ci>i_Na_b</ci>
                  <ci>i_Ca_b</ci>
                  <ci>i_NaK</ci>
                  <ci>i_ns_Ca</ci>
                  <ci>I_st</ci>
               </apply>
            </apply>
         </apply>
         <apply>
            <eq/>
            <ci>dVdt</ci>
            <apply>
               <times/>
               <apply>
                  <divide/>
                  <apply>
                     <minus/>
                     <cn cellml:units="dimensionless">1</cn>
                  </apply>
                  <ci>Cm</ci>
               </apply>
               <apply>
                  <plus/>
                  <ci>i_Na</ci>
                  <ci>i_Ca_L</ci>
                  <ci>i_Ca_T</ci>
                  <ci>i_Kr</ci>
                  <ci>i_Ks</ci>
                  <ci>i_K_Na</ci>
                  <ci>i_K_ATP</ci>
                  <ci>i_to</ci>
                  <ci>i_K1</ci>
                  <ci>i_Kp</ci>
                  <ci>i_NaCa</ci>
                  <ci>i_p_Ca</ci>
                  <ci>i_Na_b</ci>
                  <ci>i_Ca_b</ci>
                  <ci>i_NaK</ci>
                  <ci>i_ns_Ca</ci>
                  <ci>I_st</ci>
               </apply>
            </apply>
         </apply>
      </math>
