                                 <cn cellml:units="dimensionless">2</cn>
                                 <ci>i_NaCa</ci>
                              </apply>
                              <apply>
                                 <minus/>
                                 <ci>n_NaCa</ci>
                                 <cn cellml:units="dimensionless">2</cn>
                              </apply>
                           </apply>
                        </apply>
