<Chaste>

	<CryptSimulation2d>
		<CryptCircumference>16</CryptCircumference>
		<UseJiggledBottomCells>1</UseJiggledBottomCells>
		<Dt>0.00833333</Dt>
		<EndTime>20</EndTime>
		<UpdateCellPopulation>1</UpdateCellPopulation>
		<SamplingTimestepMultiple>10</SamplingTimestepMultiple>
		<UpdatingTimestepMultiple>1</UpdatingTimestepMultiple>
		<OutputDivisionLocations>0</OutputDivisionLocations>
		<OutputCellVelocities>0</OutputCellVelocities>
	</CryptSimulation2d>

	<MeshBasedCellPopulation-2-2>
		<UseAreaBasedDampingConstant>0</UseAreaBasedDampingConstant>
		<AreaBasedDampingConstantParameter>0.1</AreaBasedDampingConstantParameter>
		<WriteVtkAsPoints>0</WriteVtkAsPoints>
		<BoundVoronoiTessellation>0</BoundVoronoiTessellation>
		<HasVariableRestLength>0</HasVariableRestLength>
		<MeinekeDivisionSeparation>0.3</MeinekeDivisionSeparation>
		<CentreBasedDivisionRule>
			<CryptCentreBasedDivisionRule-2-2>
			</CryptCentreBasedDivisionRule-2-2>
		</CentreBasedDivisionRule>
		<DampingConstantNormal>1</DampingConstantNormal>
		<DampingConstantMutant>1</DampingConstantMutant>
		<OutputResultsForChasteVisualizer>1</OutputResultsForChasteVisualizer>
	</MeshBasedCellPopulation-2-2>

	<CellCycleModels>
		<SimpleWntCellCycleModel>
			<UseCellProliferativeTypeDependentG1Duration>0</UseCellProliferativeTypeDependentG1Duration>
			<WntStemThreshold>0.8</WntStemThreshold>
			<WntTransitThreshold>0.65</WntTransitThreshold>
			<WntLabelledThreshold>0.65</WntLabelledThreshold>
			<StemCellG1Duration>9.4</StemCellG1Duration>
			<TransitCellG1Duration>9.4</TransitCellG1Duration>
			<SDuration>7.4</SDuration>
			<G2Duration>1.4</G2Duration>
			<MDuration>0.72</MDuration>
		</SimpleWntCellCycleModel>
	</CellCycleModels>

	<SrnModels>
		<NullSrnModel>
		</NullSrnModel>
	</SrnModels>

	<CellKillers>
		<SloughingCellKiller-2>
			<SloughLength>16.4545</SloughLength>
			<SloughSides>0</SloughSides>
			<SloughWidth>10</SloughWidth>
		</SloughingCellKiller-2>
	</CellKillers>

	<SimulationModifiers>
	</SimulationModifiers>

	<Forces>
		<GeneralisedLinearSpringForce-2-2>
			<MeinekeSpringStiffness>30</MeinekeSpringStiffness>
			<MeinekeDivisionRestingSpringLength>0.5</MeinekeDivisionRestingSpringLength>
			<MeinekeSpringGrowthDuration>0.72</MeinekeSpringGrowthDuration>
			<UseCutOffLength>0</UseCutOffLength>
			<CutOffLength>1.79769e+308</CutOffLength>
		</GeneralisedLinearSpringForce-2-2>
	</Forces>

	<CellPopulationBoundaryConditions>
		<CryptSimulationBoundaryCondition-2>
		<UseJiggledBottomCells>1</UseJiggledBottomCells>
		</CryptSimulationBoundaryCondition-2>
	</CellPopulationBoundaryConditions>

	<NumericalMethod>
		<ForwardEulerNumericalMethod-2-2>
			<UseAdaptiveTimestep>0</UseAdaptiveTimestep> 
			<UseUpdateNodeLocation>0</UseUpdateNodeLocation> 
			<GhostNodeForcesEnabled>0</GhostNodeForcesEnabled> 
		</ForwardEulerNumericalMethod-2-2>
	</NumericalMethod>

</Chaste>
